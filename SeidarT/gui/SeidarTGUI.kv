#:kivy 1.11.1 

#Properties of the SeidarTGUI window.
<TotalLayout>:
    name: "total_layout"
    background_color: (2.9,2.9,2.9,2.9)
    tab_height: 25

#PRJ info Tab ------------------------------------------------------
<ImageInputFields>:
    name: "image_input"
    pos_hint: {"x": 0.005, "y": 0.47}
    size_hint: 0.52, 0.52

    TextInput:
        id: file_path
        name: "file_name"
        hint_text: "Enter the name of your image"
        font_size: self.height * 0.65
        multiline: False
        halign: 'left'
        valign: 'top'
        background_color: 1, 1, 1, 1
        foreground_color: 0, 0, 0, 1
        hint_text_color: 0, 0, 0, 0.5
        padding: self.width * 0.02, self.height * 0.1, 0, 0
        pos_hint: {"x":0, "y":0.9}
        size_hint: 0.71, 0.1
        on_text_validate: root.getImage()

    Button:
        id: file_input
        name: "import_image_button"
        text: "Import Image"
        font_size: min(self.width * 0.14, self.height * 0.5)
        text_size: self.size
        multiline: False
        halign: 'center'
        valign: 'middle'
        pos_hint: {"x":0.72, "y":0.9}
        size_hint: 0.28, 0.1
        on_release: root.getImage()

<GlacierImage>:
    size_hint: 1, .9
    id: image
    name: "glacier_image"

<DimensionButtons>:
    name: "dimension_button"
    pos_hint: {'x': 0.55, 'y': 0.93}
    size_hint: 0.4, 0.06    

    RelativeLayout:        
        Label:
            name: "lbl_dimbutton"
            text: "Dimensions to be plotted:"
            bold: False
            color: 0,0,0,1
            font_size: min(self.height * 0.45, self.width * 0.08)
            multiline: False
            text_size: self.size
            halign: "right"
            valign: "middle"
            pos_hint: {'x': 0, 'y': 0}
            size_hint: 0.6, 1
        
        ToggleButton:
            name: "dim_two"
            text: '2'
            font_size: min(self.width * 0.4, self.height * 0.6)
            text_size: self.size
            multiline: False
            halign: 'center'
            valign: 'middle'
            pos_hint: {'x': 0.62, 'y':0}
            size_hint: 0.15, 1
            group: "dimension"
            state: "down"

        ToggleButton:
            name: "dim_twofive"
            text: "2.5"
            font_size: min(self.width * 0.4, self.height * 0.6)
            text_size: self.size
            multiline: False
            halign: 'center'
            valign: 'middle'
            pos_hint: {"x": 0.8, "y":0}
            size_hint: 0.15, 1
            group: "dimension"
            #remove when 2.5 is implemented
            disabled: 1

